		ТегДляДекларации = DOMDocument.createDocumentFragment();
		ТегДляПриложений = DOMDocument.createDocumentFragment();
		
		LINKED_DOCS_D = DOMDocument.createElement("LINKED_DOCS");
		LINKED_DOCS_A = DOMDocument.createElement("LINKED_DOCS");
		
		ТегДляДекларации.appendChild(LINKED_DOCS_D);
		ТегДляПриложений.appendChild(LINKED_DOCS_A);
		
		// ДЛЯ ДЕКЛАРАЦИИ - ССЫЛКИ НА ВСЕ ФОРИМРУЕМЫЕ ПРИЛОЖЕНИЯ
		СтрокиПриложений = мтбСхемы.НайтиСтроки(Новый Структура("Приложение", Истина)); 
		Если СтрокиПриложений.Количество() = 0 Тогда
			мВыгружатьПакетом = Ложь;
		КонецЕсли;
		
		NUM = 1; TYPE = 1; // ссылка на приложение
		Для Каждого СтрокаПриложения Из СтрокиПриложений Цикл
			Данные = СтрокаПриложения.ФайлВыгрузки;
			
			DOC = DOMDocument.createElement("DOC");
