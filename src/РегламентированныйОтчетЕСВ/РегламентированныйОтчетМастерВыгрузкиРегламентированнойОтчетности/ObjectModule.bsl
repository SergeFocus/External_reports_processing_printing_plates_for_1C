				DOC.setAttribute("NUM",  Формат(NUM, "ЧГ=0"));
				DOC.setAttribute("TYPE", Формат(TYPE, "ЧГ=0"));
				
				Эл = DOMDocument.createElement("C_DOC");
				Эл.text = Сред(Данные,15,3);
				DOC.appendChild(Эл);
				
				Эл = DOMDocument.createElement("C_DOC_SUB");
				Эл.text = Сред(Данные,18,3);
				DOC.appendChild(Эл);
				
				Эл = DOMDocument.createElement("C_DOC_VER");
				Эл.text = Число(Сред(Данные,21,2));
				DOC.appendChild(Эл);
				
				Эл = DOMDocument.createElement("C_DOC_TYPE");
				Эл.text = Число(Сред(Данные,24,2));
				DOC.appendChild(Эл);
				
				Эл = DOMDocument.createElement("C_DOC_CNT");
				Эл.text = Формат(Число(Сред(Данные,26,7)), "ЧГ=");
				DOC.appendChild(Эл);
				
				Эл = DOMDocument.createElement("C_DOC_STAN");
				Эл.text = Сред(Данные,23,1);
				DOC.appendChild(Эл);
				
				Эл = DOMDocument.createElement("FILENAME");
				Эл.text = Данные;
				DOC.appendChild(Эл);
			
			NUM = NUM + 1;
		КонецЦикла;	
		
		// ДЛЯ ПРИЛОЖЕНИЙ - ССЫЛКА НА ДЕКЛАРАЦИЮ
		СтрокаДекларации = мтбСхемы.Найти(Истина, "Декларация"); 
