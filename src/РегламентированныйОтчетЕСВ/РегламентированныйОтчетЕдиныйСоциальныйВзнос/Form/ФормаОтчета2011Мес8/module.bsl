				УстановитьЗначениеПоказателя("П3_1_3", СтрокаБаз.СуммаВзноса);	
			КонецЕсли;
			
			Если СтрокаБаз.СтатьяНалоговойДекларации = СтрокиДекларации.ЕСВ_ФОТИнвалиды Тогда
				УстановитьЗначениеПоказателя("П2_1_4", СтрокаБаз.База);
				УстановитьЗначениеПоказателя("П3_1_4", СтрокаБаз.СуммаВзноса);	
			КонецЕсли;
			
			Если СтрокаБаз.СтатьяНалоговойДекларации = СтрокиДекларации.ЕСВ_ФОТУТОГ Тогда
				УстановитьЗначениеПоказателя("П2_1_5", СтрокаБаз.База);
				УстановитьЗначениеПоказателя("П3_1_5", СтрокаБаз.СуммаВзноса);	
