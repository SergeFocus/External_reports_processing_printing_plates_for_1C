			Если СтрокаБаз.СтатьяНалоговойДекларации = СтрокиДекларации.ЕСВ_ФОТ Тогда
				УстановитьЗначениеПоказателя("П2_1_1", СтрокаБаз.База);
				УстановитьЗначениеПоказателя("П3_1_1", СтрокаБаз.СуммаВзноса);	
			КонецЕсли;
			
			Если СтрокаБаз.СтатьяНалоговойДекларации = СтрокиДекларации.ЕСВ_ФОТБюджетники Тогда
				УстановитьЗначениеПоказателя("П2_1_2", СтрокаБаз.База);
				УстановитьЗначениеПоказателя("П3_1_2", СтрокаБаз.СуммаВзноса);	
			КонецЕсли;
			
			Если СтрокаБаз.СтатьяНалоговойДекларации = СтрокиДекларации.ЕСВ_ФОТЛетчики Тогда
